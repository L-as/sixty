data List a where
  Nil : List a
  Cons : a -> List a -> List a

f : forall a. List a -> List a
f = \as.
  case as of
    Nil ->
      as

    Nil -> -- overlapping patterns error expected
      as

    Cons x xs ->
      as

    Cons x xs -> -- overlapping patterns error expected
      as

